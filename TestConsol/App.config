<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="aws" type="Amazon.AWSSection, AWSSDK" />
  </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <add key="AWS.ContentBucketName" value="content.reservasi.com" />
    <add key="AWS.SiteMapDefaultDir" value="SiteMap" />
    <add key="AWS.SiteMapFileCount" value="100000" />
    <add key="Local.SiteMapDefaultDir" value="C:\inetpub\Global\Global.Sitemap" />
  </appSettings>
  <aws profileName="S3ACCESS" profilesLocation="D:\aws\credentials">
  </aws>
</configuration>